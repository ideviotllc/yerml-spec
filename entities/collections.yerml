%YERML 0.1
---

import "base.yerml"

# Collections

Set: := r"(${SetItem}[ \t]*,[ \t]*$?)*${SetItem}[ \t]*$"
  items: + SetItem := r"${name}"
    name: := r".+"

Enum: := r"(${EnumItem}[ \t]*,?[ \t]*$?)*${EnumItem}[ \t]*$"
  items: + EnumItem := r"${value}:[ \t]*${name}"
      value: UInt
      name: := r".+"
